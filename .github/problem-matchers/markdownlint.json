{
    "problemMatcher": [
        {
            "owner": "markdownlint",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^([^:]*):\\s*(\\d+):?(\\d+)?\\s+([\\w-\\/]*)\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "code": 4,
                    "message": 5
                }
            ]
        }
    ]
}
